<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                             http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>GfePlus</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>ObjectifyFilter</filter-name>
    <filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>ObjectifyFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>EndpointsServlet</servlet-name>
    <servlet-class>com.google.api.server.spi.EndpointsServlet</servlet-class>
    <init-param>
      <param-name>services</param-name>
      <param-value>com.protostar.prostudy.gf.service.GFBookStockService,com.protostar.prostudy.gf.service.GFCourierService,com.protostar.prostudy.gf.service.GFStudentService,com.protostar.prostudy.gf.service.PartnerSchoolService,com.protostar.prostudy.proadmin.services.ProtostarAdminService,com.protostar.prostudy.proadmin.services.UploadURLService,com.protostar.prostudy.service.AuthorizationService,com.protostar.prostudy.service.BookService,com.protostar.prostudy.service.ChapterService,com.protostar.prostudy.service.DivisionService,com.protostar.prostudy.service.InstituteService,com.protostar.prostudy.service.PracticeExamResultService,com.protostar.prostudy.service.PracticeExamService,com.protostar.prostudy.service.QuestionService,com.protostar.prostudy.service.ScheduledExamResultService,com.protostar.prostudy.service.ScheduledExamService,com.protostar.prostudy.service.ScheduledQuestionService,com.protostar.prostudy.service.ScheduledStudentExamService,com.protostar.prostudy.service.StandardService,com.protostar.prostudy.service.StudentService,com.protostar.prostudy.service.SubjectService,com.protostar.prostudy.service.UploadURLService,com.protostar.prostudy.service.UserService</param-value>
    </init-param>
    <init-param>
      <param-name>restricted</param-name>
      <param-value>false</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>EndpointsServlet</servlet-name>
    <url-pattern>/_ah/api/*</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>
        com.protostar.prostudy.until.AppServletContextListener
    </listener-class>
  </listener>  
  <servlet>
    <description></description>
    <display-name>UploadLogoServlet</display-name>
    <servlet-name>UploadLogoServlet</servlet-name>
    <servlet-class>com.protostar.prostudy.proadmin.services.UploadLogoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UploadLogoServlet</servlet-name>
    <url-pattern>/UploadLogoServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>BlobDocServe</display-name>
    <servlet-name>BlobDocServe</servlet-name>
    <servlet-class>com.protostar.prostudy.until.BlobDocServe</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>UploadCourseListServlet</display-name>
    <servlet-name>UploadCourseListServlet</servlet-name>
    <servlet-class>com.protostar.prostudy.service.UploadCourseListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UploadCourseListServlet</servlet-name>
    <url-pattern>/UploadCourseListServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>UploadDirectoryUserListServlet</display-name>
    <servlet-name>UploadDirectoryUserListServlet</servlet-name>
    <servlet-class>com.protostar.prostudy.service.UploadDirectoryUserListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UploadDirectoryUserListServlet</servlet-name>
    <url-pattern>/UploadDirectoryUserListServlet</url-pattern>
  </servlet-mapping>  
  <servlet>
    <description></description>
    <display-name>BusinessDataMigrationServlet</display-name>
    <servlet-name>BusinessDataMigrationServlet</servlet-name>
    <servlet-class>com.protostar.prostudy.until.BusinessDataMigrationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BusinessDataMigrationServlet</servlet-name>
    <url-pattern>/BusinessDataMigrationServlet</url-pattern>
  </servlet-mapping>
</web-app>